    <!-- Alpine.js Script -->
    <script>
        function appData() {
            return {
                darkMode: localStorage.getItem('darkMode') === 'true',
                activeTab: 'v7-updated',
                expanded: {
                    // v7-updated tab diagrams (collapsible with buttons)
                    '1-ugyfelfelvitel-folyamat-v2': false,
                    '1-ugyfelfelvitel-dontesi-fa-v2': false,
                    '1-ugyfelfelvitel-erd-v2': false,
                    // Features tab diagrams (collapsible - start collapsed)
                    'Feature-Hetvege-Unnepnap-Kezeles-Flowchart': false,
                    'Feature-Kartya-Kaucio-Flowchart': false,
                    'Feature-Listar-Kedvezmeny-Flowchart': false,
                    'Feature-Torzsvendeg-Szemelyazonosit√°s-Flowchart': false,
                    'Feature-Multi-Location-Kiadas-Flowchart': false,
                    'Feature-Kauci√≥-Visszatart√°s-Flowchart': false,
                    // Core tab diagrams (collapsible - start collapsed)
                    '00-kgc-erp-master-flow-2025-12-12': false,
                    '2-ertekesites-folyamat': false,
                    '2-ertekesites-erd': false,
                    '4-szerviz-folyamat': false,
                    '4-szerviz-erd-2025-12-11': false,
                    '5-penzugy-folyamat': false,
                    '7-ertesitesek-folyamat': false,
                    // Architecture tab diagrams (collapsible - start collapsed)
                    '8-deployment-architektura': false,
                    '11-multi-tenant-sema-struktura-2025-12-11': false
                },
                notes: JSON.parse(localStorage.getItem('diagramNotes') || '{}'),

                init() {
                    this.$watch('darkMode', value => {
                        localStorage.setItem('darkMode', value);
                    });
                },

                saveNotes() {
                    localStorage.setItem('diagramNotes', JSON.stringify(this.notes));
                },

                exportNotes() {
                    let csv = 'Diagram,Note\n';
                    for (const [key, value] of Object.entries(this.notes)) {
                        if (value) {
                            csv += `"${key}","${value.replace(/"/g, '""')}"\n`;
                        }
                    }
                    const blob = new Blob([csv], { type: 'text/csv' });
                    const url = URL.createObjectURL(blob);
                    const a = document.createElement('a');
                    a.href = url;
                    a.download = 'kgc-erp-v7-notes.csv';
                    a.click();
                    URL.revokeObjectURL(url);
                }
            };
        }
    </script>
</body>
</html>
